{
  "name": "wso-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "dom-to-image": "^2.6.0",
    "google-client-api": "^1.0.4",
    "jwt-decode": "^3.1.2",
    "prop-types": "^15.8.1",
    "react": "^16.8.6",
    "react-date-picker": "^7.8.1",
    "react-dom": "^16.8.6",
    "react-ga": "^2.7.0",
    "react-google-charts": "^3.0.14",
    "react-icons": "^3.10.0",
    "react-modal": "^3.15.1",
    "react-redux": "^7.2.8",
    "react-router-dom": "6",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.13.4",
    "react-table": "^7.8.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.0.5",
    "serialize-javascript": "3.1.0",
    "timeago.js": "^4.0.0-beta.2",
    "typeface-source-sans-pro": "^0.0.75",
