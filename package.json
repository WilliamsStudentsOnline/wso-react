    "react-redux": "^7.1.0",
    "react-router5": "^7.1.0",
    "react-scripts": "3.2.0",
    "redux": "^4.0.1",
    "redux-router5": "^7.0.2",
    "redux-saga": "^1.0.5",
    "router5": "^7.0.2",
    "router5-plugin-browser": "^7.0.2",
    "timeago.js": "^4.0.0-beta.2",
    "typeface-source-sans-pro": "^0.0.75"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.4",
    "babel-eslint": "^10.0.3",
    "deep-freeze": "^0.0.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-utils": "1.4.1",
    "glob-all": "^3.1.0",
    "husky": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-junit": "^6.4.0",
    "lint-staged": "^9.0.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "purgecss-webpack-plugin": "^1.6.0",
    "react-test-renderer": "^16.8.6",
    "redux-logger": "^3.0.6",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "scripts": {
    "analyze": "node scripts/analyze",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "purgecss --config ./purgecss.config.js --out build/static/css",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "jest-junit": {
    "output": "coverage/junit/junit.xml",
    "usePathForSuiteName": "true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "./node_modules/.bin/eslint --ext .jsx,js --ignore-path .gitignore --fix",
      "pretty-quick --staged",
      "git add"
    ]
  },
  "proxy": "http://localhost:8080"
}
